<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">

    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if0103.assessment.ui.hitung.HitungFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_tentangFragment"
            app:destination="@+id/tentangFragment"/>
        <action
            android:id="@+id/action_hitungFragment_to_historiFragment"
            app:destination="@+id/historiFragment"/>
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if0103.assessment.ui.AboutFragment"
        android:label="AboutFragment"
        tools:layout="@layout/fragment_about"/>
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if0103.assessment.ui.histori.HistoriFragment"
        android:label="@string/histori"
        tools:layout="@layout/fragment_histori"/>
    <fragment
        android:id="@+id/tentangFragment"
        android:name="org.d3if0103.assessment.ui.tentang.TentangFragment"
        android:label="Tentang"
        tools:layout="@layout/fragment_tentang"/>
</navigation>